<Window x:Class="logistic.AdminMain"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:logistic"
        mc:Ignorable="d"
        Title="Администрирование" Height="600" Width="1050">
    <Grid Background="#DDC5A2">
        <Grid.RowDefinitions>
            <RowDefinition Height="90"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="20"/>
        </Grid.RowDefinitions>
        <TextBlock Text="Администрирование" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <Grid Background="#9B4F0F" Panel.ZIndex="-2"/>
        <Grid Background="#8D230F" Grid.Row="2"/>
        <Frame NavigationUIVisibility="Hidden" ContentRendered="MainFrame_ContentRender" Grid.Row="1" Name="MainFrame"/>
        <Button x:Name="Btn_Back" Content="Назад" VerticalAlignment="Top" Click="Btn_Back_Click" Margin="0,10,10.2,0" Width="175" HorizontalAlignment="Right"/>
        <Button x:Name="Btn_Exit" Content="Выход" VerticalAlignment="Top" Click="Btn_Exit_Click" Margin="0,50,10.2,0" Width="175" HorizontalAlignment="Right"/>
    </Grid>
</Window>
